{
  "name": "drizzle-pokemon",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/src/index.js",
    "migrations:generate": "drizzle-kit generate:pg",
    "migrations:push": "node dist/src/migrate.js",
    "migrations:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "seed": "node dist/src/seed.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/index.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.1.4",
    "drizzle-orm": "^0.26.5",
    "express": "^4.18.2",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.18.1",
    "nodemon": "^1.14.9",
    "typescript": "^5.1.3"
  }
}
